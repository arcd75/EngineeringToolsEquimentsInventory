function productCompareClick(pID,pName,pIMAGE){
	var inList=false;
	for(i=0;i<productCompareList.length;i++){
		if(productCompareList[i][0]==pID){
			inList=true;
		}
	}
	if(inList==false){
		productCompareList.push(Array(pID,pName,pIMAGE));
		$('#productCompare_'+pID).addClass('oncompared');
		sortCompareProduct();
		$.post("../comparesession.php",{PID:pID,ACTION:'add'});	
	}
}
function sortCompareProduct(){
	if(productCompareList.length>0){
		$('#compareProductHolder').css({'display':'block'});
		$('#compareProductHolder').animate({'opacity':1},1000);
	} else {
		$('#compareProductHolder').animate({'opacity':0},1000,function(){
			$('#compareProductHolder').css({'display':'none'});
		});
	}
	arrageSortCompareProduct();
	function arrageSortCompareProduct(){
		$('.productcompare').removeClass('oncompared');
		$('#compareProductHolder div ul').html('');
		for(i=0;i<productCompareList.length;i++){
			var thisArray=Array();
			thisArray=productCompareList[i];
			$('#compareProductHolder div ul').append('<li><a href="javascript:removeSortCompareProduct('+i+','+thisArray[0]+')"><span class="compare_close"></span><img src="../upload_files/'+thisArray[2]+'" alt="'+thisArray[1]+'"><br>'+thisArray[1]+'</a></li>');
			$('#productCompare_'+thisArray[0]).addClass('oncompared');
		}
		if(productCompareList.length>=2){
			$('#compareProductHolder div ul').append('<li><a href="product_compare.php" id="compare_btn">COMPARE<br>PRODUCTS</a></li>');
		}
		if(!$('#compareProductHolder img').hasClass('closeBtn')){
			$('#compareProductHolder').append('<p class="closeBtn" onclick="closeCompareProduct()" style="position:absolute;right:0%;top:-40px;width:100px;heigtht:40px;line-height:40px;text-align:center;cursor: pointer;background-color:#36F;color:#ffffff;font-size:18px;border-radius:5px">CLOSE</p>');
		}
	}
}
function closeCompareProduct(){
	loadCompareProduct().done(function(data){
		productCompareList = [];
		for(var i=0;i<data.length;i++){
			var pID = data[i][0];
			sortCompareProduct();
			$.post("../comparesession.php",{PID:pID,ACTION:'delete'});	
		}
	})
}
function removeSortCompareProduct(num,pID){
	productCompareList.splice(num,1);
	sortCompareProduct();
	$.post("../comparesession.php",{PID:pID,ACTION:'delete'});
	$('.compare_td_'+num).remove();
	window.location.reload();
}
function sortProductList(){
	inProductSizeSearch=productIDs;
	inProductScreenSearch=productIDs;
	inProductTypeSearch=productIDs;
	inProductInputSearch=productIDs;
	$('.selectSize').bind('change',function(){
		inProductSizeSearch=Array();
		$('.selectSize').each(function(index, element) {
			if($(this).is(':checked')){
				for(i=0;i<=productSizes.length;i++){
					thisValue=$(this).attr('value');
					thisValue=thisValue.split('-');
					if(productSizes[i]>=Number(thisValue[0]) && productSizes[i]<=Number(thisValue[1])){
						inProductSizeSearch.push(productIDs[i]);
					}
				}
			}
		});
		arrangeProductList();
	});
	//$system_screen=array(1=>"16:9",2=>"16:10",3=>"4:3",4=>"5:3",5=>"5:4",6=>"21:9");
	$('.selectScreen').bind('change',function(){
		inProductScreenSearch=Array();
		$('.selectScreen').each(function(index, element) {
			if($(this).is(':checked')){
				for(i=0;i<=productScreens.length;i++){
					thisValue=$(this).attr('value');
					//if(productIDs[i]==5067){console.log(productScreens[i]+'====='+thisValue);}
					if(thisValue=='4,5' || thisValue==0){
						if(productScreens[i]==4 || productScreens[i]==5 || productScreens[i]==2 || productScreens[i]==''){
							inProductScreenSearch.push(productIDs[i]);
						}
					} else if(productScreens[i]==Number(thisValue)){
						inProductScreenSearch.push(productIDs[i]);
					} 
				}
				
			}
		});
		//console.log(inProductScreenSearch);
		arrangeProductList();
	});
	//$system_model_type=array(1=>"LED",2=>"3D",3=>"IPS",4=>"LCD",5=>"TV",6=>"AIO",7=>"4K");
	$('.selectType').bind('change',function(){
		inProductTypeSearch=Array();
		$('.selectType').each(function(index, element) {
			if($(this).is(':checked')){
				for(i=0;i<=productTypes.length;i++){
					thisValue=$(this).attr('value');
					if(in_array(',',productTypes[i])){
						productTypes[i]=String(productTypes[i]);
						productTypes[i]=productTypes[i].split(',');
					}
					//if(productIDs[i]==5140){console.log(productTypes[i]+'====='+thisValue+'===='+in_array(String(thisValue),productTypes[i]));}
					if(thisValue==0){
						if(productTypes[i]==1||productTypes[i]==2||productTypes[i]==4||productTypes[i]==5||productTypes[i]==6||productTypes[i]=='' ){
							if(!in_array(String(productIDs[i]),inProductTypeSearch)){
								inProductTypeSearch.push(productIDs[i]);
							}
						}
					}else if(in_array(String(thisValue),productTypes[i]) || String(thisValue)==productTypes[i]){
						if(!in_array(String(productIDs[i]),inProductTypeSearch)){
							inProductTypeSearch.push(productIDs[i]);
						}
					}
				}
			}
		});
		//console.log(inProductTypeSearch.length+'===='+inProductTypeSearch);
		arrangeProductList();
	});
	$('.selectInput').bind('change',function(){
		inProductInputSearch=Array();;
		$('.selectInput').each(function(index, element) {
			if($(this).is(':checked')){
				for(i=0;i<=productInputs.length;i++){
					thisValue=$(this).attr('value');
					if(in_array(',',productInputs[i])){
						productInputs[i]=String(productInputs[i]);
						productInputs[i]=productInputs[i].split(',');
					}
					if(thisValue==0 && productInputs[i]=='' && !in_array(String(productIDs[i]),inProductInputSearch)){
						inProductInputSearch.push(productIDs[i]);
					}else if(in_array(String(thisValue),productInputs[i]) && !in_array(String(productIDs[i]),inProductInputSearch)){
						inProductInputSearch.push(productIDs[i]);
					}
				}
			}
		});
		arrangeProductList();
	});
	arrangeProductList();	
}
function loadProducts(){
	return $.getJSON('productlist.json');
}
loadProducts().done(function(data){
	 setTimeout(function(){
		 $.each(data,function(key,value){
			 if(value.pid){
				productIDs.push(value.pid);
				productSeries.push(value.jy_sub_series);
				productStats.push(value.jy_status);
				productNames.push(value.modelName);
				productSubNames.push('<span class="subname">'+value.jy_name_add_new+'<br>'+value.jy_name_add+'</span><br>');
				productCategories.push(value.jy_category);
				productCIDs.push(value.cid);
				productSizes.push(value.jy_size);
				productScreens.push(value.jy_screen);
				productTypes.push(value.jy_model_type);
				productInputs.push(value.jy_model_input);
				productImages.push(value.jy_main_picture);
				productEan.push(value.jy_ean);
				productDescription.push(value.jy_description);
				productNew.push(value.jy_newProduct);
				productIcon.push(value.jy_icon);
				productKind.push(value.jy_kind);
				productSearchResult.push({value:'product_display.php?id='+value.pid,label:value.modelName});
			 }
		});
		sortProductList();
		loadCompareProduct();
	 },100);
});
