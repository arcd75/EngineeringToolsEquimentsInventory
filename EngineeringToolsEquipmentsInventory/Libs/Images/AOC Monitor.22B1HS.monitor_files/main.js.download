function swipBanner(num){
	if(num==0){
		$('#martian_container').css({'display':'block'});
	}else{
		$('#martian_container').css({'display':'none'});
	}
	currentBanner=num;
	var leftX=0-currentBanner*windowWidth/windowWidth*documentWidth;
	$('#home_banner_holder').animate({left:leftX},100);
	bannerCountingSecound=bannerSecound;
	onBanner();
}
function swipLeft(){
	$('#martian_container').css({'display':'none'});
	if(currentBanner!=$('#home_banner_holder ul li').length-1){
		currentBanner=currentBanner+1;
	}else{
		currentBanner!=$('#home_banner_holder ul li').length-1;
	}
	leftX=0-currentBanner*windowWidth/windowWidth*documentWidth;
	$('#home_banner_holder').animate({left:leftX},1500);
	bannerCountingSecound=bannerSecound;
	onBanner();
}
function swipRight(){
	if(currentBanner==1){
		setTimeout(function(){$('#martian_container').css({'display':'block'});},1000);
	}
	if(currentBanner!=0){
		currentBanner=currentBanner-1;
	} else {
		currentBanner!=0;
	}
	leftX=0-currentBanner*windowWidth/windowWidth*documentWidth;
	$('#home_banner_holder').animate({left:leftX},1500);
	bannerCountingSecound=bannerSecound;
	onBanner();
}
function onBanner(){
	$('#main_features_holder ul li a').each(function(index, element) {
		if(index!=currentBanner){
     	   $(this).removeClass('on_banner'); 
		} else {
			$(this).addClass('on_banner'); 
		}
    });
}
function in_array(needle,haystack,argStrict) {
  var key = '',
  strict = !! argStrict;
  if (strict) {
    for (key in haystack) {
      if (haystack[key] === needle) {
        return true;
      }
    }
  } else {
    for (key in haystack) {
      if (haystack[key] == needle) {
        return true;
      }
    }
  }
  return false;
}
