function getWindowSize(){
	windowWidth=$(window).width();
	windowHeigh=$(window).height();
	if(!detectIE()){
		if(windowWidth<=900){
			windowWidth=900;
			$('#navigator').css({'position':'absolute'});
			$('body').css({'width':documentWidth});	
		}
		zoomScale=windowWidth/documentWidth;
		$('body').css({'zoom':zoomScale});
	}else{
		addIECSS();
		zoomScale=1;
		windowWidth=1900;
	}
	//if(windowWidth<=800){windowWidth=800; $('#navigator').css({'position':'absolute'});}
	//else if(isIE==false){$('#navigator').css({'position':'fixed'});}
	//if(!detectIE()){$('#contenter').css({'zoom':windowWidth/documentWidth});}else{addIECSS();}
	subMenuWidth=Number($('.sub_nav_list li a').innerWidth());
	subMenuNumber=Number($('.sub_nav_list li').length);
	subMenuLeft=Number((documentWidth-subMenuWidth*subMenuNumber)/2)+50;
	totalBanner=$('#home_banner_holder ul li').length;
	$('#home_banner_holder').css({'width':windowWidth*totalBanner/windowWidth*documentWidth});
	$('#home_banner_holder img').css({'width':windowWidth/zoomScale});
	$('#main_features_holder ul li a').css({'width':100/$('#main_features_holder ul li a').length+'%'});
	$('#main_icons_holder').css({'width':$('#main_icons_holder ul li').length*110});
	$('#main_quick_link_holder').css({'width':$('#main_quick_link_holder .main_quick_link_holder_col').length*360});
	$('#product_category_holder').css({'width':$('#product_category_holder a').length*192});
}
