function loadCompareProduct(){
	return $.getJSON('../comparesession.php');;
}
loadCompareProduct().done(function(data){
	if(productCompareDisplay==true){
		setTimeout(function(){
			if(data.length>=1 && data[0]!=''){
				 $.each(data,function(key,value){
					 var arrayNumber=Number(0);
					 for(i=0;i<=productIDs.length;i++){
						 if(productIDs[i]==value){
							 arrayNumber=i;
							 break;
						 }
					 }
					 productCompareClick(value,productNames[arrayNumber],productImages[arrayNumber])
				});
				sortProductList();
			}
		 },100);
	}
});
function loadProductImages(){
